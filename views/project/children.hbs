<div class="department-table">
    {{> messages }}
    <table class="table table-striped">
        <div class="container">
            <thead>
            <th>Project Name</th>
            </thead>
            <tbody>
            {{#if projects}}
                {{#each projects}}
                    <tr>
                        <td>{{this.name}}</td>
                    </tr>
                {{/each}}
            {{/if}}
            </tbody>
        </div>
    </table>
</div>

<script src="/assets/js/jquery.min.js"></script>
<script src="/assets/js/project-children.js"></script>